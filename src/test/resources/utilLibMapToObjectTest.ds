local languages = [
    {
      "name": "Foo",
      "language": {
        "name": "Java",
        "version": "1.8"
      }
    },
    {
      "name": "Bar",
      "language": {
        "name": "Scala",
        "version": "1.0"
      }
    },
    {
      "name": "FooBar",
      "language": {
        "name": "Java",
        "version": "1.7"
      }
    }
  ];

local expected = {
                 "Java": {
                   "name": "Java",
                   "version": "1.7"
                 },
                 "Scala": {
                   "name": "Scala",
                   "version": "1.0"
                 }
               };

local mapped = tro.objectFrom(languages, function(x) x.language.name, function(v) v.language);

assert expected == mapped : "MapToObject result is " + mapped;

true
